    "type" : "<%- type %>"
    ,"id"  : "<%- id %>"
    ,"label" : "<%= label %>"
    ,"options" : [<% _.each(options, function(option, idx, list) { %>
	      {
		  "label": "<%= option %>"
		  ,"value": "<%= option.toLowerCase() %>"
	      }<% if(idx !== list.length-1)  { %>,<% } %> <% }) %>
    ]
